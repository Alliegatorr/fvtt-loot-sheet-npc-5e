{
  "name": "fvtt-loot-sheet-npc-5e",
  "version": "1.0.0",
  "description": "A javascript FoundryVTT module. Adding an additional NPC sheet for Loot/Containers/Merchant",
  "main": "lootsheetnpc5e.js",
  "scripts": {
    "local-all": "npm run less && npm run copyfiles && npm run localcopy && npm run generate-docs",
    "all": "npm run less && npm run copyfiles ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copyfiles": "copyfiles ./lootsheetnpc5e.js ./module.json ./changelog.md ./readme.md ./modules/**/* ./template/**/* ./packs/**/* ./icons/**/* ./dist",
    "less": "lessc -clean-css ./styles/lootsheetNPC5e.less ./dist/styles/lootsheetNPC5e.css",
    "generate-docs": "jsdoc -c jsdoc.json",
    "localcopy": "copyfiles -u 1 ./dist/**/* /Users/jackp/AppData/Local/FoundryVTT/Data/modules/lootsheetnpc5e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielBoettner/fvtt-loot-sheet-npc-5e.git"
  },
  "keywords": [
    "javascript",
    "less",
    "css",
    "FoundryVTT",
    "module"
  ],
  "author": "Jan Ole Peek (ChalkOne)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanielBoettner/fvtt-loot-sheet-npc-5e/issues"
  },
  "homepage": "https://github.com/DanielBoettner/fvtt-loot-sheet-npc-5e#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "generate-docs": "^1.1.1",
    "less-plugin-clean-css": "^1.5.1",
    "lessc": "^1.0.2"
  }
}
